<% include header.ejs %>

<!--博客内容-->
<div class="blog">
    <div class="blog-container">
        <!--文章列表 start-->
        <div class="article-left">
            <!--1-->
            <div class="article-list">
                <!--标题-->
                <div class="article-list-title"><h2><%= post.title %></h2></div>
                <!--提示信息-->
                <div class="article-list-tips">
                    <ul>
                        <li><span>日期：</span><span><%= post.date %></span></li>
                        <li><span>作者：</span><span><%= post.name %></span></li>
                        <li><span>类型：</span><span><%= post.category %></span></li>
                        <!--<li><span>浏览：</span><span>121</span></li>-->
                    </ul>
                </div>
                <!--内容-->
                <div class="article-list-info">
                    <span><%= post.post %></span>
                </div>
            </div>
            <div class="underline"></div>
            <!-- UY BEGIN -->
            <div id="uyan_frame"></div>
            <script type="text/javascript" src="http://v2.uyan.cc/code/uyan.js?uid=2095106"></script>
            <!-- UY END -->
        </div>
        <!--文章列表 end-->

        <!--右侧栏 start-->
        <div class="article-right">
            <!--最新文章 start-->
            <div class="article-later">
                <dl>
                    <dt>最新文章</dt>
                    <dd><a href="javascript:;" target="_blank">HTML5</a></dd>
                    <dd><a href="javascript:;" target="_blank">CSS</a></dd>
                    <dd><a href="javascript:;" target="_blank">Javascript</a></dd>
                    <dd><a href="javascript:;" target="_blank">node</a></dd>
                    <dd><a href="javascript:;" target="_blank">MongoDb</a></dd>
                </dl>
            </div>
            <!--最新文章 end-->
            <div class="hr-20"></div>
            <!--类型标签页 start-->
            <div class="article-type">
                <dl>
                    <dt>标签</dt>
                    <dd><a href="javascript:;">HTML5</a></dd>
                    <dd><a href="javascript:;">CSS</a></dd>
                    <dd><a href="javascript:;">Javascript</a></dd>
                    <dd><a href="javascript:;">node</a></dd>
                    <dd><a href="javascript:;">MongoDb</a></dd>
                    <dd><a href="javascript:;">HTML5</a></dd>
                    <dd><a href="javascript:;">CSS</a></dd>
                    <dd><a href="javascript:;">Javascript</a></dd>
                    <dd><a href="javascript:;">node</a></dd>
                    <dd><a href="javascript:;">MongoDb</a></dd>
                    <dd><a href="javascript:;">HTML5</a></dd>
                    <dd><a href="javascript:;">CSS</a></dd>
                    <dd><a href="javascript:;">Javascript</a></dd>
                    <dd><a href="javascript:;">node</a></dd>
                    <dd><a href="javascript:;">MongoDb</a></dd>
                </dl>
            </div>
            <!--类型标签页 end-->
            <div class="hr-20"></div>
            <!--预留广告位 start-->
            <div class="article-ad">
                广告
            </div>
            <!--预留广告位 end-->
        </div>
        <!--右侧栏 end-->
    </div>
</div>

<% include footer.ejs %>
